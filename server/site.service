[Unit]
Description=booker daemon
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/opt/inout_dump
Restart=on-failure
RestartSec=5s
ExecStart=/opt/inout_dump/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/opt/inout_dump/site.sock config.wsgi

[Install]
WantedBy=multi-user.target
